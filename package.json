{
  "name": "travel-packing-list",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "homepage": "https://flanzana.github.io/travel-packing-list",
  "scripts": {
    "predeploy": "rm -rf dist",
    "deploy": "vite build && gh-pages -d dist",
    "dev": "vite",
    "prebuild": "yarn types && yarn biome && yarn test",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "types": "tsc",
    "biome": "biome check --error-on-warnings",
    "biome:fix": "biome check --error-on-warnings --write"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.31.0",
    "@emotion/react": "^11.14.0",
    "i18next": "^25.4.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "gh-pages": "^6.1.1",
    "jsdom": "^26.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.5.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
